<section id="teaser">
  <div class="container">
    <div class="row">
        <div class="col-lg-12 text-center">
            {{ content }}
        </div>
    </div>
	{% for teaser in page.header.teasers %}
		<div class="row">
			<video controls="1" alt="{{teaser.mediaFile}}" width="100%">
				<source src="{{ page.media[teaser.mediaFile].url }}">
				Your browser does not support the video tag.
			</video> 
		</div>
	{% endfor %}
</div>
</section>
